    def get_task_num(self) -> int: ...
    def get_task_progress(self) -> int: ...
    def get_result_num(self) -> int: ...
    def get_last_result(self) -> Seed: ...
    def get_results(self) -> list[Seed]: ...

class CheckBatchManager:
    def __init__(self, start_seed:int, end_seed:int, start_star_num:int, end_star_num:int, galaxy_condition:dict, quick:bool, max_thread:int): ...
    def run(self) -> None: ...
